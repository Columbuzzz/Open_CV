   "source": [
    "import cv2\n",
    "#read webcam\n",
    "webcam = cv2.VideoCapture(0)\n",
    "#visualize webcam\n",
    "\n",
    "while True:\n",
    "    ret , frame = webcam.read()\n",
    "\n",
    "    cv2.imshow('frame',frame)\n",
    "    if cv2.waitKey(40) & 0xFF == ord('q'):\n",
    "        break\n",
    "\n",
    "webcam.release()\n",
    "cv2.destroyAllWindows()"
   ]
